{
  "name": "payload-cms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "migrate": "payload migrate",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "ci": "payload migrate && pnpm run build",
    "heroku-postbuild": "cross-env NODE_ENV=production pnpm run generate:types && pnpm run generate:importmap && pnpm run build",
    "render-build": "pnpm run generate:types && pnpm run generate:importmap && pnpm run build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.37.0",
    "@payloadcms/email-resend": "3.37.0",
    "@payloadcms/next": "3.37.0",
    "@payloadcms/payload-cloud": "3.37.0",
    "@payloadcms/plugin-seo": "3.37.0",
    "@payloadcms/richtext-lexical": "3.37.0",
    "@payloadcms/storage-s3": "3.37.0",
    "@payloadcms/storage-uploadthing": "3.37.0",
    "@payloadcms/translations": "3.37.0",
    "@payloadcms/ui": "3.37.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/uuid": "^10.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cron-parser": "^5.3.0",
    "cross-env": "^7.0.3",
    "fast-xml-parser": "^5.2.5",
    "got": "^14.4.7",
    "graphql": "^16.8.1",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.544.0",
    "next": "15.2.6",
    "next-themes": "^0.4.6",
    "node-cron": "^4.2.1",
    "p-limit": "^6.2.0",
    "payload": "3.37.0",
    "postcss": "^8.5.6",
    "react": "19.0.1",
    "react-dom": "19.0.1",
    "recharts": "2.15.4",
    "sharp": "0.32.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "uuid": "^11.1.0",
    "xml-flow": "^1.0.4",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@payloadcms/db-sqlite": "3.37.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.5.4",
    "@types/react": "^19.0.12",
    "@types/react-dom": "19.0.4",
    "@types/xml-flow": "^1.0.4",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.2.3",
    "prettier": "^3.4.2",
    "react-router-dom": "^7.5.0",
    "supabase": "^2.39.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
